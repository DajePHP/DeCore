{
    "name": "daje/de-core",
    "type": "library",
    "description": "The DajePHP core",
    "keywords": [
        "framework"
    ],
    "homepage": "http://dajephp.org",
    "license": "MIT",
    "authors": [
        {
            "name": "Giulio De Donato",
            "email": "liuggio@gmail.com"
        },
        {
            "name": "Giorgio Cefaro",
            "email": "me@giorgiocefaro.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/DajePHP/FastRouteMiddleware"
        }
    ],
    "require": {
        "php": ">=5.4",
        "psr/log": "~1.0",
        "stack/builder": "*",
        "willdurand/stack-negotiation": "*",
        "silpion/stack-logger": "*",
        "asm89/stack-cors": "*",
        "symfony/http-kernel": "*",
        "tombzombie/dice": "*",
        "nikic/fast-route": "*",
        "DajePHP/FastRouteMiddleware": "*"
    },
    "require-dev": {
        "behat/behat": "~3"
    },
    "config": {
        "bin-dir": "bin/"
    },
    "autoload": {
        "psr-4": {
            "Daje\\HttpKernel\\": "src/"
        }
    },
    "minimum-stability": "dev"
}
